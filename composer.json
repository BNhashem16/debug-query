{
    "name": "bnhashem/debug-query",
    "description": "This is my package debug-query",
    "keywords": [
        "Bnhashem",
        "laravel",
        "debug-query"
    ],
    "homepage": "https://github.com/Bnhashem16/debug-query",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mohamed Hashem",
            "email": "me@aBnhashem.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.0|^7.0|^6.0|^5.5",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Bnhashem\\DebugQuery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bnhashem\\DebugQuery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnhashem\\DebugQuery\\DebugQueryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
